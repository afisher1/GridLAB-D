// test checkpoints
#set checkpoint_type=SIM
#set checkpoint_interval=86400

clock {
	starttime '2000-01-01 0:00:00';
	stoptime '2000-01-07 0:00:00';
}

class test {
	random x;
};
module assert;
object test:..2 {
	x "type:uniform(0,1); refresh:1min";
	object assert {
		target x;
		relation inside;
		lower 0;
		upper 1;
	};
}