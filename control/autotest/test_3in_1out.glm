// $Id$

clock {
	timezone PST+8PDT;
	starttime '2000-01-01 00:00:00 PST';
	stoptime '2000-01-02 00:00:00 PST';
}


class test {
	random x;
	random y;
	random z;
	double w;
};

module tape;
object test {
	name test;
	x "type:normal(0,1); integrate; refresh:1min";
	y "type:normal(0,1); integrate; refresh:1min";
	z "type:normal(0,1); integrate; refresh:1min";
	object recorder {
		file "test_w.csv";
		property "x,y,z,w";
		interval 60;
	};
}

module control;

object ss_model {
	form OCF;
	timestep 1 min;
	Y "0.1 0.2 0.05";
	U "0.25 0.6 0.15";
	x "0 0 0";
	u "test.x test.y test.z";
	y "test.w";
}