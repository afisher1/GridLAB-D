#set randomseed=10

#define stylesheet="C:\Project Files\Gridlabd\gridlab-d\trunk\VS2005\gridlabd-2_0.xsl"

clock {
	timezone PST+8PDT;
	starttime '2001-01-01 00:00:00 PST';
	stoptime '2001-01-15 00:00:00 PST';
}

module residential {
	implicit_enduses NONE;
}

module assert;
module tape;
module climate;

object house{

	cooling_setpoint 100;
	heating_setpoint 1;
	
	object recorder{
		property mass_temperature,air_temperature;
		file "test_solargains.csv";
		interval 3600;
		limit 10000;
	};	

	object double_assert {
		target "mass_temperature";
		value 60;
		within 40;
	};
	
	object double_assert {
		target "air_temperature";
		value 60;
		within 35;
	};
}

object climate{

	tmyfile "../Olympia.tmy2";

}

