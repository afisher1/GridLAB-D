
//Purpose: To test the energy calculations of the dishwasher. The default power factor
//for the diswasher is .95. All calculations were hand checked using a calculator.
//
//Note: Last hand check passed 07/20/2009
//

clock {
	timestamp '2000-01-01 0:00:00';
	stoptime '2000-01-01 24:00:00';
	timezone PST+8PDT;
}

module tape;
module residential;
module assert;

object house:1 {
	floor_area random.normal(1500,300) sf;
	heating_setpoint 10;
	cooling_setpoint 150;
	air_temperature 74;
	mass_temperature 74;
	thermostat_deadband 2;
}

object dishwasher:2 {
	parent house:1;
	installed_power 2000W;
	object complex_assert {
		target "energy_meter";
		within .1;
		object player {
			property value;
			file "..\test_dishwasher_energy.player";
		};
	};
	object recorder{
		property enduse_load,internal_gains,energy_meter;
		file "test_dishwasher_energy.csv";
		interval -1;
		limit 100;
	};	
}

object player{
	parent dishwasher:2;
	property demand;
	file "..\generic_demand.glt";
	loop 1;
}