#set savefile="pnnl14bus.xml"

clock {
	timestamp '2000-01-01 0:00:00';
}

////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// modules
module network {
	acceleration_factor 1.0;
	convergence_limit 0.001;
}

////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// PNNL 14 Bus Test
//

import network pnnl14bus.cdf;

object generator {
	parent node.type:SWING;
	Pmax_MW 1000;
	Qmax_MVAR 100;
	Qmin_MVAR -100;
}

#ifdef TAPE
module tape;
object player {
	file ${TAPE};
	loop 50000;
}
#endif
