//Delta-Delta Transformer Self-Test
//
//Tests power in and power out of a three phase
//  Delta-Delta Transformer, with a balanced load attached.

#set iteration_limit=100

clock {
	timezone EST+5EDT;
	starttime '2000-01-01 00:00:00 EST';
}

module powerflow;
module assert;

///////////////////////////////////////////////////////
// DELTA-DELTA TRANSFORMER
///////////////////////////////////////////////////////
// ABCD

object transformer {
	phases ABCD;	
	configuration object transformer_configuration {
		connect_type DELTA_DELTA;
		install_type VAULT;
		primary_voltage 7200 V;
		secondary_voltage 4160 V;
		power_rating 6000;
		powerA_rating 2000;
		powerB_rating 2000;
		powerC_rating 2000;
		resistance 0.01;
		reactance 0.06;
	};
	from object node {
		name XFMR_fABCD;		
		phases ABCD;
		voltage_A 7200 + 0j;		
		voltage_B -3600 - 6235.383j;		
		voltage_C -3600 + 6235.383j;
		nominal_voltage 7200.00;		
	};
	to object load {
		name XFMR_tABCD;
		phases ABCN;
		voltage_A 4160 +0j;
		voltage_B -2080 - 3602.666j;
		voltage_C -2080 + 3602.666j;		
		constant_current_A 1;
		constant_current_B -0.5-0.866j;
		constant_current_C -0.5+0.866j;
		constant_power_A 1000+2000j;	
		constant_power_B 1000+2000j;	
		constant_power_C 1000+2000j;	
		constant_impedance_A 1500+750j;		
		constant_impedance_B 1500+750j;		
		constant_impedance_C 1500+750j;	
		nominal_voltage 4160.00;
	};
	object double_assert {
		target "power_out";
		value 47504.417;
		within 0.5;
	};		
	object double_assert {
		target "power_in";
		value 47508.699;
		within 0.5;
	};	
};
